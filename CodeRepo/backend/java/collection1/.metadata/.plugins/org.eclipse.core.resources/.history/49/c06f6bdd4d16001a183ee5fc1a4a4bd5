package com.capgmini.customer.app;

import java.util.Scanner;

public class HomePage {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		while(true) {
			System.out.println("1.Contractor\n2.Customer\n3.Product\n4.Home");

			int check=sc.nextInt();
			switch(check) {
			case 1:Contractor.contractor(args);
			break;
			case 2:Customer.customer(args);
				break;
			case 3:Product.main(args);
				break;
			case 4:Product.main(args);
				break;
			default:
				break;
			}
		}		}
}
