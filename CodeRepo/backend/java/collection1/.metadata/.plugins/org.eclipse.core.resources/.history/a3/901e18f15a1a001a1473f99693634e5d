package com.caggemini.hibernateapp.dto;


@Entity
@Table(name="movie")
public class Movie {
	@id
	@Column
	private int id;
	@Column
	private String name;
	private String rating;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getRating() {
		return rating;
	}
	public void setRating(String rating) {
		this.rating = rating;
	}
	
	
}
